{
  "name": "tamo",
  "displayName": "tamo",
  "description": "tamo game",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.103.0"
  },
  "categories": [
    "Other"
  ],
 "activationEvents": [
    "onCommand:tamo.showView",
    "onCommand:tamo.sendMessage"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [{
      "command": "tamo.showView",
      "title": "Tamagotchi Live View"
